{
  "name": "strapi-middleware-cache",
  "version": "2.1.7",
  "description": "Cache strapi requests",
  "main": "./lib",
  "types": "./types",
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "run-s eslint:fix prettier:fix",
    "test": "run-s test:*",
    "test:tsc": "tsc",
    "test:lint": "run-s eslint prettier",
    "test:mocha": "mocha ./tests/*.test.js --exit"
  },
  "keywords": [
    "cache",
    "strapi",
    "requests",
    "redis",
    "memcache"
  ],
  "directories": {
    "lib": "./lib",
    "types": "./types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrixr/strapi-middleware-cache.git"
  },
  "author": "Patrick R",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "koa-router": "^8.0.8",
    "lodash": "^4.17.15",
    "path-to-regexp": "^6.2.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/lodash": "^4.14.176",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.31.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.0.3",
    "koa": "^2.11.0",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.0",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "sinon": "^9.2.4",
    "strapi-supercharged": "0.3.2",
    "supertest": "^4.0.2",
    "supertest-koa-agent": "^0.3.2",
    "typescript": "^4.3.5",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
